  "description": "Write data to a file, replacing the file if it already exists and creating any intermediate directories if they don't already exist. Thin wrapper around node's native fs methods.",
  "engines": {
    "node": ">=4"
  },
  "homepage": "https://github.com/jonschlinkert/write",
  "keywords": [
    "file",
    "filepath",
    "files",
    "filesystem",
    "folder",
    "fs",
    "fs.writeFile",
    "fs.writeFileSync",
    "path",
    "write"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "write",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/write.git"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "delete",
        "read-data",
        "read-yaml",
        "write-data",
        "write-json",
        "write-yaml"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.0.3"
}
